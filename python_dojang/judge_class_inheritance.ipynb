        "class Animal:\n",
        "  def eat(self):\n",
        "    print('eat')\n",
        "\n",
        "class Wing:\n",
        "  def flat(self):\n",
        "    print('flat')\n",
        "\n",
        "class Bird(Animal, Wing):\n",
        "  def fly(self):\n",
        "    print('fly')\n",
        "\n",
        "b = Bird()\n",
        "b.eat()\n",
        "b.flat()\n",
        "b.fly()\n",
        "print(issubclass(Bird, Animal))\n",
        "print(issubclass(Bird, Wing))"
      ]
